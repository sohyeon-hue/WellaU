tyle","_createCommentVNode","floatPopBoxOpen","_hoisted_4","_hoisted_5","_hoisted_6","_hoisted_7","_hoisted_8","_toDisplayString","floatBoxType","onClick","floatPopClse","_hoisted_9","_hoisted_10","_hoisted_11","_createTextVNode","_hoisted_12","_hoisted_13","_hoisted_14","_component_AlertBox","list","alertLists","_hoisted_15","_hoisted_16","_component_StepBox","stepList","step","stepStep","_hoisted_17","_hoisted_18","_hoisted_19","_component_ProfileBox","name","conText","img","img1","_hoisted_20","_hoisted_21","_hoisted_22","_component_IconButtonBox","icon","disabled","bDis","battery","iconBattery","iconButtonBoxClick","aDis","iconButtonBoxClickAlert","rDis","iconButtonBoxClickRecord","_component_BothBar","content","_withCtx","_hoisted_23","rightArea","_component_a_switch","checked","switchChecked","_cache","$event","_","_hoisted_24","king","img2","birthday","img3","_hoisted_25","_component_a_button","default","_hoisted_26","_hoisted_27","checked1","_hoisted_28","_hoisted_29","checked2","_component_PcLnb","open","lnbOpen","group","person","friends","lnbList","onGropClick","gropClick","onLogout","logout","_component_ModalBox","modalShow","padding","_component_ListSelectBox","value","listSelectBoxValue","title","buttonText","outLine","listSelectBoxList","onListClick","listSelectBoxListClick","buttonShow"],"sources":["/Users/gimgaui/Documents/documents/vue/WellaU/WellaU/src/views/pages/pcView.vue"],"sourcesContent":["<template>\n    <div class=\"content-wrapper full\">\n        <div class=\"content-scroll\">\n            <div class=\"mapArea dummy-back\">\n                <MapPin type=\"pie\" :photo=\"photo\" :per=\"per\" style=\"position:absolute; top:50%; left:50%;\" />\n                <!-- pop 알림 & 위치기록  -->\n                <div class=\"floating-right-top-box\" v-if=\"floatPopBoxOpen\">\n                    <div class=\"floating-right-top-con-wrapper\">\n                        <div class=\"floating-right-top-con\">\n                            <!-- 제목 -->\n                            <div class=\"top\">\n                                <div class=\"tit\">{{floatBoxType === 'alert' ? '알림목록' : '위치기록' }}</div>\n                                <!-- 닫기 버튼 -->\n                                <i @click=\"floatPopClse\"><CloseOutlined /></i>\n                            </div>\n                            <!-- 위치 기록 시 입력폼  -->\n                            <div class=\"middle\" v-if=\"floatBoxType === 'record'\">\n                                <div class=\"mark-time-box\">\n                                    <div class=\"date\">\n                                        <i><CalendarOutlined /></i>2024년 09월 13일\n                                    </div>\n                                    <div class=\"time\">\n                                        <i><ClockCircleOutlined /></i>13시\n                                    </div>\n                                </div>\n                            </div>\n                            <!-- 알림 콘텐츠 -->\n                            <div class=\"con-wrapper\" v-if=\"floatBoxType === 'alert'\">\n                                <div class=\"con\">\n                                    <AlertBox :list=\"alertLists\" />\n                                </div>\n                            </div>\n                            <!-- STEP 콘텐츠 -->\n                            <div class=\"con-wrapper\" v-else>\n                                <div class=\"con\">\n                                    <StepBox :list=\"stepList\" :step=\"stepStep\" />\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"floating-menu-box\">\n                    <div class=\"floating-menu-wrapper\">\n                        <div class=\"top\">\n                            <ProfileBox name=\"홍길동\" conText=\"서울시 용산동 갈월동 | 13분전\" :img=\"img1\"></ProfileBox>\n                        </div>\n                        <div class=\"con-wrapper\">\n                            <div class=\"con\">\n                                <div class=\"btn-set horizontal\">\n                                    <IconButtonBox icon=\"battery\" :disabled=\"bDis\" :battery=\"iconBattery\"  @click=\"iconButtonBoxClick\" />\n                                    <IconButtonBox icon=\"alert\" :disabled=\"aDis\"  @click=\"iconButtonBoxClickAlert\" />\n                                    <IconButtonBox icon=\"record\" :disabled=\"rDis\"  @click=\"iconButtonBoxClickRecord\" />\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <BothBar style=\"display:none;\">\n            <template #content>\n                <div class=\"both-bar-area\">\n                    <ProfileBox name=\"홍길동\" conText=\"서울시 용산동 갈월동 | 13분전\" :img=\"img1\">\n                        <template #rightArea>\n                            <a-switch v-model:checked=\"switchChecked\" />\n                        </template>\n                    </ProfileBox>\n                </div>\n                <div class=\"both-bar-area\">\n                    <ProfileBox name=\"김응삼\" conText=\"서울시 용산동 갈월동 | 13분전\" king :img=\"img2\"></ProfileBox>\n                    <ProfileBox name=\"김은지\" conText=\"서울시 용산동 갈월동 | 13분전\" birthday :img=\"img3\"></ProfileBox>\n                </div>\n                <div class=\"both-bar-area paddingNone\">\n                    <a-button type=\"link\" class=\"addGroupPersonBtn\">+ 그룹원 추가 하기</a-button>\n                </div>\n                <div class=\"title-info-style\">\n                    <div class=\"l\">\n                        <div class=\"tit\">내 프로필 공개</div>\n                        <div class=\"info\">그룹원에게 내 프로필 사진을 공개합니다.  비활성 시 기본 프로필 아이콘이 노출됩니다.</div>\n                    </div>\n                    <div class=\"r\"><a-switch v-model:checked=\"checked1\" /></div>\n                </div>\n                <div class=\"title-info-style\">\n                    <div class=\"l\">\n                        <div class=\"tit\">내 연락처 공개</div>\n                        <div class=\"info\">그룹원에게 내 연락처를 공개합니다. 비활성 시 그룹장을 제외한 그룹원에게 연락처가 노출되지 않으며 통화연결이 되지 않습니다.</div>\n                    </div>\n                    <div class=\"r\"><a-switch v-model:checked=\"checked2\" /></div>\n                </div>\n            </template>\n        </BothBar>\n        <PcLnb v-model:open=\"lnbOpen\" :group=\"group\" :person=\"person\" :friends=\"lnbList\" @gropClick=\"gropClick\" @logout=\"logout\" />\n    </div>\n\n    <!-- modal -->\n    <ModalBox v-model:modalShow=\"modalShow\" :padding=\"0\">\n        <ListSelectBox v-model:value=\"listSelectBoxValue\" title=\"그룹을 선택해주세요.\" buttonText=\"+ 그룹 만들기\" :outLine=\"false\" :list=\"listSelectBoxList\" @listClick=\"listSelectBoxListClick\" :buttonShow=\"false\" />\n    </ModalBox>\n</template>\n<script setup>\n    import { ref } from 'vue'\n    import { CloseOutlined,CalendarOutlined,ClockCircleOutlined } from '@ant-design/icons-vue'\n\n    const checked1 = ref(false);\n    const checked2 = ref(false);\n    const switchChecked = ref(false);\n\n    const img1 = ref({\n        src : require('@img/dummy-profile01.jpeg'),\n        alt : '홍길동님의 프로필 사진입니다'\n    })\n    const img2 = ref({\n        src : require('@img/dummy-profile02.jpeg'),\n        alt : '김응삼 프로필 사진입니다'\n    })\n    const img3 = ref({\n        src : require('@img/dummy-profile03.jpeg'),\n        alt : '김은지 프로필 사진입니다'\n    })\n\n    /* MAP PIN */\n    const photo = ref({\n        src : require('@img/dummy-profile01.jpeg'),\n        alt : '홍길동님의 프로필 사진입니다'\n    })\n    const per = ref(Math.floor(Math.random()*100));\n\n    /* BUTTON */\n    const iconButtonBoxClickAlert = ()=>{\n        if(floatBoxType.value === 'alert' && floatPopBoxOpen.value){\n            floatPopBoxOpen.value = false;\n        }else{\n          