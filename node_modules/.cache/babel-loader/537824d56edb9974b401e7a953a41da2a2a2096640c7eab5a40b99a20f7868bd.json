{"ast":null,"code":"import { ref, computed } from 'vue';\nexport default {\n  __name: 'BothBar',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const points = ref({\n      s: -1,\n      e: -1,\n      h: 100,\n      min: 100,\n      cuh: 100\n    });\n    const targetChecked = target => {\n      const cl = target.getAttribute(\"class\");\n      const checked = cl.match(/ant-/g);\n      const parent = target.parentNode;\n      const parentCl = parent.getAttribute(\"class\");\n      const parehtChecked = parentCl.match(/ant-/g);\n      return checked || parehtChecked ? true : false;\n    };\n    const touchStart = $ev => {\n      console.log($ev.target.getAttribute(\"class\"));\n      targetChecked($ev.target);\n      points.value.s = $ev.targetTouches[0].pageY;\n      points.value.cuh = points.value.h;\n      // $ev.preventDefault();\n    };\n    const touchMoved = $ev => {\n      const touchPoint = $ev.targetTouches[0].pageY;\n      const diff = points.value.s - touchPoint;\n      const calc = points.value.cuh + diff;\n      points.value.h = calc;\n    };\n    const touchEnd = () => {\n      points.value.s = -1;\n    };\n    const bothBarWrapperStyle = computed(() => {\n      const style = {};\n      style.height = points.value.h + \"rem\";\n      return style;\n    });\n    const __returned__ = {\n      points,\n      targetChecked,\n      touchStart,\n      touchMoved,\n      touchEnd,\n      bothBarWrapperStyle,\n      ref,\n      computed\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","points","s","e","h","min","cuh","targetChecked","target","cl","getAttribute","checked","match","parent","parentNode","parentCl","parehtChecked","touchStart","$ev","console","log","value","targetTouches","pageY","touchMoved","touchPoint","diff","calc","touchEnd","bothBarWrapperStyle","style","height"],"