     min-height:100rem;\n        overflow:hidden;\n        background:#fff;\n    }\n    .bothBar-thumn{\n        position:relative;\n        height:30rem;\n        background:#000;\n        &:after{\n            content:\"\";\n            position:absolute;\n            top:50%;\n            left:50%;\n            width:60rem;\n            height:5rem;\n            border-radius:5rem;\n            transform:translate(-50%,-50%);\n            background:#ededed;\n        }\n    }\n    .bothBar-content{\n        padding:20rem;\n    }\n</style>"],"mappings":"AAYI,SAASA,GAAG,EAACC,QAAQ,QAAQ,KAAI;;;;;;;IACjC,MAAMC,MAAM,GAAGF,GAAG,CAAC;MAACG,CAAC,EAAC,CAAC,CAAC;MAACC,CAAC,EAAC,CAAC,CAAC;MAACC,CAAC,EAAC,GAAG;MAACC,GAAG,EAAC,GAAG;MAACC,GAAG,EAAC;IAAG,CAAC,CAAC;IACrD,MAAMC,aAAa,GAAIC,MAAM,IAAG;MAC5B,MAAMC,EAAE,GAAGD,MAAM,CAACE,YAAY,CAAC,OAAO,CAAC;MACvC,MAAMC,OAAO,GAAGF,EAAE,CAACG,KAAK,CAAC,OAAO,CAAC;MACjC,MAAMC,MAAM,GAAGL,MAAM,CAACM,UAAU;MAChC,MAAMC,QAAQ,GAAGF,MAAM,CAACH,YAAY,CAAC,OAAO,CAAC;MAC7C,MAAMM,aAAa,GAAGD,QAAQ,CAACH,KAAK,CAAC,OAAO,CAAC;MAC7C,OAAOD,OAAO,IAAIK,aAAa,GAAG,IAAI,GAAG,KAAK;IAClD;IACA,MAAMC,UAAU,GAAIC,GAAG,IAAG;MACtBC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACV,MAAM,CAACE,YAAY,CAAC,OAAO,CAAC;MAC5CH,aAAa,CAACW,GAAG,CAACV,MAAM,CAAC;MACzBP,MAAM,CAACoB,KAAK,CAACnB,CAAC,GAAGgB,GAAG,CAACI,aAAa,CAAC,CAAC,CAAC,CAACC,KAAK;MAC3CtB,MAAM,CAACoB,KAAK,CAACf,GAAG,GAAGL,MAAM,CAACoB,KAAK,CAACjB,CAAC;MACjC;IACJ;IACA,MAAMoB,UAAU,GAAIN,GAAG,IAAG;MACtB,MAAMO,UAAU,GAAGP,GAAG,CAACI,aAAa,CAAC,CAAC,CAAC,CAACC,KAAK;MAC7C,MAAMG,IAAI,GAAGzB,MAAM,CAACoB,KAAK,CAACnB,CAAC,GAAGuB,UAAU;MACxC,MAAME,IAAI,GAAG1B,MAAM,CAACoB,KAAK,CAACf,GAAG,GAAGoB,IAAG;MACnCzB,MAAM,CAACoB,KAAK,CAACjB,CAAC,GAAGuB,IAAI;IACzB;IACA,MAAMC,QAAQ,GAAGA,CAAA,KAAI;MACjB3B,MAAM,CAACoB,KAAK,CAACnB,CAAC,GAAG,CAAC,CAAC;IACvB;IACA,MAAM2B,mBAAmB,GAAG7B,QAAQ,CAAC,MAAI;MACrC,MAAM8B,KAAK,GAAG,CAAC,CAAC;MAChBA,KAAK,CAACC,MAAM,GAAG9B,MAAM,CAACoB,KAAK,CAACjB,CAAC,GAAG,KAAK;MACrC,OAAO0B,KAAI;IACf,CAAC","ignoreList":[]},"metadata":{},"sourceType