headerText = \"\";\n          break;\n        case \"groupMain\" :\n          this.headerData.type = \"type5\";\n          this.headerText = \"그룹1\";\n          break;\n        default : \n          this.headerData.type = \"type1\";\n          this.headerText = \"\";\n          break;\n      }\n    }\n  },\n  watch : {\n    $route(to) {\n      let path = (to.fullPath.replace(/(^\\/)/,'') === \"\")?\"listPage\":to.fullPath.replace(/(^\\/)/,'');\n      this.$store.state.pageStatus = path;\n    },\n    pageStatus(n,o){\n      if(n !== o) this.checkePath(n);\n    },\n  },\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  height:100%;\n}\n.app-wrapper{\n  display:flex;\n  flex-direction:column;\n  height:100%;\n  #content{\n    flex:1;\n    /* overflow:auto; */\n    &[page=listPage]{\n      overflow:auto;\n    }\n  }\n}\n</style>\n"],"mappings":"AAyBA,SAAQA,GAAG,QAAO,KAAI;AACtB,OAAOC,UAAS,MAAO,yBAAwB;AAC/C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAI,EAAI,CAAC,CAAC;EACVC,KAAKA,CAAA,EAAE;IACL,MAAMC,UAAS,GAAIL,GAAG,CAAC,EAAE,CAAC;IAC1B,OAAO;MACLK;IACF;EACF,CAAC;EACDC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,QAAO,EAAI;IACTC,UAAUA,CAAA,EAAE;MACV,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,UAAS;IACpC,CAAC;IACDG,WAAWA,CAAA,EAAE;MACX,IAAIC,IAAG,GAAI,KAAK;MAChB,IAAG,IAAI,CAACJ,UAAS,KAAM,OAAM,IAAK,IAAI,CAACA,UAAS,KAAM,WAAW,EAAC;QAChEI,IAAG,GAAI,IAAI;MACb;MACA,OAAOA,IAAI;IACb,CAAC;IACDC,SAASA,CAAA,EAAE;MACT,IAAID,IAAG,GAAI,IAAI;MACf,IAAG,IAAI,CAACJ,UAAS,KAAM,UAAS,IAAK,IAAI,CAACA,UAAS,KAAM,OAAO,EAAC;QAC/DI,IAAG,GAAI,KAAI;MACb;MACA,OAAOA,IAAI;IACb;EACF,CAAC;EACDE,IAAG,EAAIA,CAAA,KAAI;IACT,OAAO;MACLC,OAAM,EAAI,IAAI;MACdC,UAAS,EAAI;QACXC,UAAS,EAAI,IAAI;QACjBC,IAAG,EAAI,OAAO;QACdC,IAAG,EAAI,IAAI;QACXC,KAAI,EAAI,EAAE;QACVC,OAAM,EAAI,IAAI;QACdC,QAAO,EAAI,EAAE;QACbC,SAAQ,EAAI,EAAE;QACdC,QAAO,EAAI;MACb;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAE,CAAC,CAAC;EACXC,OAAOA,CAAA,EAAE;IACP,IAAI,CAACC,UAAU,CAAC